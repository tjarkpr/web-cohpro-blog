{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/*"
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/notfound"
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }